    <div class="site-section bg-light">
      <form  action="/filtercar"  method="post"  id="filter">
  <div class="container-fluid"><div class="float-right pr-5" style="border-radius: 15px;"><a href="" data-toggle="modal" data-target="#exampleModal" ><span class="btn btn-warning"><strong class="">Filter</span><img src="https://cdn-icons.flaticon.com/png/512/3024/premium/3024539.png?token=exp=1654518729~hmac=73ad51f2d58ea4ed05b0519d00c89e9d" style="width:20px;height: 22px;" class="mt-1 ml-2"></strong></a></div></div>
      <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel"><b>Category</b></h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
       <div class="row">
          <div class="col-4 boxes"><b style="padding-left:16px;">All</b><input type="checkbox" name="filter" value="All" ></div>
         <div class="col-4 boxes"><b>Hatchback</b><input type="checkbox" name="filter"  value="hatchback"></div>
         <div class="col-4 boxes"><b></b><input type="checkbox" name="filter"  value="" hidden checked></div>
         <div class="col-4 boxes "><b style="padding-left:32px;">SUV</b><input type="checkbox" name="filter" value="suv"> </div>
        <div class="col-4 boxes"><b>MUV</b><input type="checkbox" name="filter"  value="muv"> </div>
        <div class="col-4 boxes"><b style="padding-left:30px;">Sedan</b><input type="checkbox" name="filter" value="sedan"></div>
         <div class="col-4 boxes"><b>Premium</b><input type="checkbox" name="filter" value="premium" ></div>
       </div> </div> 
      <div class="modal-footer">
        
        <button type="submit" class="btn btn-primary">Filter</button>
        </form>
      </div>
    </div>
  </div>
</div>


      <div class="container">
        <div class="row">
          <div class="col-lg-7">
            <h2 class="section-heading"><strong> Cars available at your Location</strong></h2>
            <p class="mb-5">Life Is Too Short To Drive Boring Cars</p>
          </div>
        </div>


        <div class="row " id="reloadpath">{{#each cars}}
          <div class="col-md-6 col-lg-4 mb-4">

            <div class="listing d-block  align-items-stretch">
              <div class="listing-img  mr-4">
                <img src="/images/productimages/{{this._id}}(3).jpg" alt="Image" class="img-fluid">
              </div>
              <div class="listing-contents h-100">
                <h3>{{this.brandname}}
                </h3>
                <h3>{{this.brandmodel}}
                </h3>
    
                <div class="d-block d-md-flex mb-3 border-bottom pb-3">
                  <div class="listing-feature pr-4">
                    <span class="caption">Seats:</span>
                    <span class="number">{{this.seats}}</span>
                  </div>
                  <div class="listing-feature pr-4">
                    <span class="caption">Fuel:</span>
                    <span class="number">{{this.fuel}}</span>
                  </div>
                  <div class="listing-feature pr-4">
                    <span class="caption">Transmission:</span>
                    <span class="number "><b>{{this.transmission}}</b></span>
                  </div>
                </div>
                <div>
                   
                  <p><a href="/rent/{{this._id}}" class="btn btn-primary btn-sm">Rent Now</a></p>
                </div>
              </div>
            </div>
          </div>{{/each}}

          
          
          
             
    




        </div>
      </div>
