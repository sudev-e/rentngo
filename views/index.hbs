    }
    
    el_parents.addEventListener('change', function populate_child(e){
        el_childs.innerHTML = '';
        itm = e.target.value;
        if(itm in mList){
                for (i = 0; i < mList[itm].length; i++) {
                    el_childs.innerHTML = el_childs.innerHTML + '<option>'+ mList[itm][i] +'</option>';
                }
        }
    });

    
    
    </script>
